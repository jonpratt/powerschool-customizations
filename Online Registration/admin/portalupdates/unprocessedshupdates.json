[~[tlist_sql;

SELECT
   U_PORTALUPDATE.STUDENTSDCID as StudentDCID,
   U_PORTALUPDATE.ID as UpdateID,
   U_PORTALUPDATE.WHENCREATED as UpdateDate,
   STUDENTS.LASTFIRST as StudentName,
   CASE WHEN U_STUDENT_DEMOGRAPHICS.HASSECONDARYHOUSEHOLD=1 THEN U_SECONDARY_HOUSEHOLD.ID ELSE -1 END as SecondaryHouseholdID

FROM
   U_PORTALUPDATE
    
INNER JOIN
   STUDENTS on U_PORTALUPDATE.STUDENTSDCID=STUDENTS.DCID

INNER JOIN
   U_STUDENT_DEMOGRAPHICS on U_STUDENT_DEMOGRAPHICS.STUDENTSDCID=STUDENTS.DCID

LEFT OUTER JOIN
   U_SECONDARY_HOUSEHOLD on U_SECONDARY_HOUSEHOLD.STUDENTSDCID=STUDENTS.DCID   
   
WHERE
   U_STUDENT_DEMOGRAPHICS.SHUPDATEPENDING=1 AND
   U_PORTALUPDATE.SHUPDATEPENDING=1
        
ORDER BY U_PORTALUPDATE.WHENCREATED
]

    {"studentDCID":"~(StudentDCID)",
     "updateID":"~(UpdateID;js)",
     "updateDate":"~(UpdateDate;js)",
     "studentName":"~(StudentName;js)",
     "secondaryHouseholdID":"~(SecondaryHouseholdID;js)"
    },

[/tlist_sql]{}]